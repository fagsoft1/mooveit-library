{
  "presets": [
    "@babel/preset-env",
    "@babel/react",
    [
      "@babel/preset-typescript",
      {
        "allowDeclareFields": true
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components"
        }
      }
    ]
  ]
}